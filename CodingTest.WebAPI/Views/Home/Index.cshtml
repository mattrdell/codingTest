@section Scripts {
    @Scripts.Render("~/KnockoutModels/LoginViewModel")
}
<div class="row">
    @Html.Partial("_Register");
    @Html.Partial("_Login");
</div>
<div class="row">
    <div class="col-md-8">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h2 class="panel-title">Gilded Rose Online Store</h2>
            </div>
            <div class="panel-body" data-bind="with: ItemListViewModel">
                @Html.Partial("_ItemList")
            </div>
        </div>
    </div>

</div>




